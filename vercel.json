{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    },
    {
      "src": "/game.js",
      "dest": "/game.js"
    },
    {
      "src": "/node_modules/phaser/dist/phaser.min.js",
      "dest": "/node_modules/phaser/dist/phaser.min.js"
    }
  ]
}